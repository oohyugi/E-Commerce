<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.yogi.ecommerce.feature.home.ui.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_feed"
        android:name="com.yogi.ecommerce.feature.feed.FeedFragment"
        android:label="Feed"
        tools:layout="@layout/fragment_feed" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.yogi.ecommerce.feature.carts.CartsFragment"
        android:label="Carts"
        tools:layout="@layout/fragment_carts" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.yogi.ecommerce.feature.purchased.PurchasedFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_purchased" />
</navigation>